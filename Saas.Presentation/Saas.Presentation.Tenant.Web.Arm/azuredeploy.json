{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "serverFarmName": {
      "type": "string",
      "defaultValue": "contoso-dev-appsp"
    },
    "serverFarmResourceGroup": {
      "type": "string",
      "defaultValue": "contoso-dev-foundation"
    },
    "webAppName": {
      "type": "string"
    }
  },
  "variables": {
    "name": "[parameters('webAppName')]"
  },
  "resources": [
    {
      "name": "[variables('name')]",
      "type": "Microsoft.Web/sites",
      "location": "eastus",
      "apiVersion": "2015-08-01",
      "dependsOn": [],
      "tags": {
        "[concat('hidden-related:', resourceId(parameters('serverFarmResourceGroup'), 'Microsoft.Web/serverFarms', parameters('serverFarmName')))]": "Resource",
        "displayName": "ma-subs"
      },
      "properties": {
        "name": "[variables('name')]",
        "serverFarmId": "[resourceId(parameters('serverFarmResourceGroup'), 'Microsoft.Web/serverFarms', parameters('serverFarmName'))]"
      },
      "resources": [
        {
          "name": "MSDeploy",
          "type": "extensions",
          "location": "[resourceGroup().location]",
          "apiVersion": "2015-08-01",
          "dependsOn": [ "[resourceId('Microsoft.Web/sites', variables('name'))]" ],
          "tags": { "displayName": "deploy" },
          "properties": {
            "packageUri": "https://contosodevstorageacct.blob.core.windows.net/packages/eventsAppPackage.zip",
            "dbType": "",
            "connectionString": ""
            //"setParameters": {
            //  "IIS Web Application Name": "[variables('name')]"
            //}
          }
        }
      ]
    }
  ],
  "outputs": {}
}